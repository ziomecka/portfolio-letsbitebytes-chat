    "webpack": "4.39.2",
    "webpack-cli": "3.2.3",
    "webpack-merge": "4.2.2"
  },
  "scripts": {
    "build": "webpack --config webpack/build/index.js",
    "build:watch": "npm run build -- --watch",
    "deploy": "webpack --config webpack/deploy/index.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "nodemon": "nodemon",
    "start": "ts-node --project tsconfig.json --files src/back/index.ts",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "tsc": "tsc -p tsconfig.json",
    "tsc:watch": "npm run tsc -- -w",
    "work": "run-p nodemon build:watch test:watch tsc:watch"
  },
  "keywords": [
    "React",
    "typescript",
    "socket.io"
  ],
  "author": "Katarzyna Ziomek-Zdanowicz",
  "license": "ISC",
  "repository": "https://github.com/ziomecka/call-the-trainer.git",
  "engines": {
    "npm": "6.4.1",
    "node": "12.3.0"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      "build"
    ],
    "exec": "npm start",
    "ext": "ts,tsx,js,jsx"
  }
}
