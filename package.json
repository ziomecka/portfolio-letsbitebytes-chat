  "scripts": {
    "build": "webpack --config webpack/build/index.js",
    "build:watch": "npm run build -- --watch",
    "heroku-postbuild": "npm run deploy",
    "deploy": "webpack --config webpack/deploy/index.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "nodemon": "nodemon --config .nodemon.json",
    "nodemon:start": "ts-node --project tsconfig.json --files src/back/index.ts",
    "postinstall": "npm run tsc:compile",
    "start": "node --optimize_for_size tsc/src/back/index.js $PORT",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "tsc:compile": "rm -rf tsc && tsc -p tsconfig.json",
    "tsc:watch": "npm run tsc:compile -- -w",
    "work": "run-p nodemon build:watch test:watch tsc:watch",
    "maintenance:sync:usersCache": "ts-node --project tsconfig.json -r dotenv/config --files src/back/maintenance/index.ts syncUsersCache",
    "maintenance:delete:users": "ts-node --project tsconfig.json -r dotenv/config --files src/back/maintenance/index.ts deleteUsers",
    "maintenance:create:defaultUsers": "ts-node --project tsconfig.json -r dotenv/config --files src/back/maintenance/index.ts createDefaultUsers",
    "maintenance:restore:testDB": "npm run maintenance:delete:users && npm run maintenance:create:defaultUsers && npm run maintenance:sync:usersCache",
    "maintenance:update:defaultUsers": "ts-node --project tsconfig.json -r dotenv/config --files src/back/maintenance/index.ts updateDefaultUsers"
  },
  "keywords": [
    "React",
    "typescript",
    "socket.io"
  ],
